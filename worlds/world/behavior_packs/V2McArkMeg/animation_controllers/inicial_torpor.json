    "controller.animation.inicial_torpor_allosaurus": { 
      "initial_state": "default",
      "states": { 
        "default": {
          "transitions": [ 
            {
              "lvl10": "(query.has_any_family('adult_lvl10')) && !query.is_tamed && !query.is_baby"
            },
            { 
              "lvl50": "(query.has_any_family('adult_lvl50')) && !query.is_tamed && !query.is_baby"
            },
            {
              "lvl100": "(query.has_any_family('adult_lvl100')) && !query.is_tamed && !query.is_baby"
            }, 
            {
              "lvl150": "(query.has_any_family('adult_lvl150')) && !query.is_tamed && !query.is_baby"
            }
          ]
        }, 
			"lvl10": {
"on_entry": [
 "variable.torpor = 40; return variable.torpor;"
                    ],
          "transitions": [ 
            {
              "torpor_empty_lvl10": "(variable.torpor <= 0.0) && !query.is_tamed && !query.is_baby"
            }
 
          ]
				},
			"torpor_empty_lvl10": {
					"on_entry": [ 
"@s minecraft:narcotic_final"
					] ,
          "transitions": [ 
            {
              "default": "query.is_tamed"
            },
            {
              "lvl10": "!query.is_sheared"
            }
 
          ]
				},
				
			"lvl50": {
"on_entry": [
 "variable.torpor = 60; return variable.torpor;" 
                    ], 
          "transitions": [ 
            {
              "torpor_empty_lvl50": "(variable.torpor <= 0.0) && !query.is_tamed && !query.is_baby"
