    "controller.animation.rage_effect_coc": {    
      "initial_state": "default",
      "states": { 
			"default": { 
          "transitions": [ 
            {
              "default_t": "!query.is_baby" 
            }
 
          ]
				}, 
			"default_t": { 
					"on_entry": [
					    
"@s minecraft:v1_rage_off"
					] ,
          "transitions": [ 
            {
              "on":"variable.bufo_carcha >= 10.0"
            }
            
 
          ]
				},
			"on": { 
					"on_entry": [
					    
"@s minecraft:v1_rage_on"
					] ,
          "transitions": [ 
            {
              "default_t":"variable.bufo_carcha < 10.0"
            }
 
          ]
				}
      }  
    },

    "controller.animation.puntos_animales_pequenos": {
      "initial_state": "default",
      "states": {
        "default": {
          "transitions": [   
            {
              "on": "!query.is_tamed && !query.is_baby && !query.is_sheared && query.skin_id == 1"
            }
          ]
        }, 
				"on": {
                    "animations": [
            {
              "puntos_animales_pequenos_mc_ark_animation": "query.mark_variant < 4"
            },
            {
              "puntos_animales_pequenos_mc_ark_animation2": "query.mark_variant >= 4 && query.has_rider"
            }

                        ],
"on_entry": [

 "/effect @s weakness 1 25 true",
 "/effect @s slowness 1 25 true"
                    ],
          "transitions": [  
            {
              "default": "query.skin_id == 0"
            }  
          ]
				}
      }  
    },
