{
	"format_version": "1.10.0",
	"minecraft:attachable": {
		"description": {
			"identifier": "pa:steel_chestplate.player",
			"item": { "pa:steel_chestplate": "query.owner_identifier == 'minecraft:player'" },
			"materials": {
				"default": "armor",
				"enchanted": "armor_enchanted"
			},
			"textures": {
				"default": "textures/models/steel_chestplate",
				"enchanted": "textures/misc/enchanted_item_glint"
			},
			"geometry": {
				"default": "geometry.steel_chestplate"
			},
			"scripts": {
				"parent_setup": "variable.chest_layer_visible = 0.0;"
			},
			"animations": {
				"offset": "animation.armor.chestplate.offset"
			},
			"render_controllers": [
				"controller.render.armor"
			]
		}
	}
}