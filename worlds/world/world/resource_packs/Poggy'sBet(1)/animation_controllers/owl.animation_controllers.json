{
	"format_version": "1.19.0",
	"animation_controllers": {
		"controller.animation.owl": {
			"states": {
				"default": {
					"animations": ["idle"],
					"transitions": [
						{"fly": "q.is_on_ground == false"}
					],
					"blend_transition": 0.2
				},
				"fly": {
					"animations": ["fly"],
					"transitions": [
						{"default": "q.is_on_ground == true"}
					],
					"blend_transition": 0.2
				}
			}
		}
	}
}