{
  "format_version": "1.10.0",
  "render_controllers": {
    "controller.render.yumi_bow": {
      "arrays": {
        "textures": {
            "array.bow_texture_frames": [
              "texture.default",
              "texture.bow_pulling_0",
              "texture.bow_pulling_1",
              "texture.bow_pulling_2"
            ]
          },
          "geometries": {
            "array.bow_geo_frames": [
              "geometry.default",
              "geometry.bow_pulling_0",
              "geometry.bow_pulling_1",
              "geometry.bow_pulling_2"
            ]
          }
        },
        "geometry": "array.bow_geo_frames[math.abs(query.item_in_use_duration) > 150 && query.main_hand_item_use_duration > 0 ? 3 : math.abs(query.item_in_use_duration / 50)]",
        "materials": [
          {
            "*": "variable.is_enchanted ? material.enchanted : material.default"
          }
        ],
        "textures": [
          "array.bow_texture_frames[math.abs(query.item_in_use_duration) > 150 && query.main_hand_item_use_duration > 0 ? 3 : math.abs(query.item_in_use_duration / 50)]",
          "texture.enchanted"
        ]
      },
      "controller.render.yumi_unloaded": {
        "geometry": "Geometry.default",
        "materials": [
          {
            "*": "variable.is_enchanted ? material.enchanted : material.default"
          }
        ],
        "textures": [
          "Texture.default",
          "texture.enchanted"
        ]
      }
    }
  }