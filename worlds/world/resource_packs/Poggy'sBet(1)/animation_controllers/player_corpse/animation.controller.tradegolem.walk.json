{
	"format_version": "1.10.0",
	"animation_controllers": {
		"controller.animation.tadegolem.walk": {
			"states": {
				"default": {
					"animations": [
						"idle"
					],
					"transitions": [
						{
							"walking": "q.modified_move_speed > 0.05"
						}
					],
					"blend_transition": 0.2
				},
				"walking": {
					"animations": [
						"walk"
					],
					"transitions": [
						{
							"default": "q.modified_move_speed < 0.05"
						}
					],
					"blend_transition": 0.2
				}
			}
		}
	}
}