							{
								"queue_command": {
									"command": [
										"structure load \"entrance_tunnel\" ~-2 ~-10 ~",
										"setblock ~ ~ ~ cobbled_deepslate_wall",
										"event entity @s pog:despawn"
									]
								}
							}
						]
